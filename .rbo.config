{
  "version": "1.0.0",
  "language": "rubolt",
  "entry": "main.rbo",
  "output": "build/",
  "strict": true,
  "typecheck": true,
  "optimize": false,
  "includes": [
    "src/**/*.rbo",
    "lib/**/*.rbo"
  ],
  "excludes": [
    "node_modules",
    "*.test.rbo"
  ],
  "compiler": {
    "warnings": true,
    "errorOnWarning": false,
    "target": "native"
  }
}
