{
  "name": "rubolt",
  "displayName": "Rubolt Language Support",
  "description": "Syntax highlighting and language support for Rubolt (.rbo) files",
  "version": "1.0.0",
  "publisher": "rubolt-team",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "rubolt",
        "aliases": [
          "Rubolt",
          "rubolt"
        ],
        "extensions": [
          ".rbo"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rubolt",
        "scopeName": "source.rubolt",
        "path": "./syntaxes/rubolt.tmLanguage.json"
      }
    ],
    "configurationDefaults": {
      "[rubolt]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rubolt/vscode-rubolt"
  }
}
